
#top {
    position: relative;
    background-color: #4682B4;
    color: white;
    padding: 0px;
    margin: 0px 0px 8px 0px;
    height: 137px
    
}

#bottom {
    position: relative;
    background-color: #c2dfef;
    padding: 5px;
    margin: 10px 0px 0px 0px;
    clear: both;
}

#left {
    float: left;
    background-color: #ece3a5;
    padding: 5px;
    width: 200px;
    height: 120px;
}

#right {
    float: right;
    background-color: #ece3a5;
    padding: 5px;
    width: 150px;
}

.center_content {
    position: relative;
    background-color: #dddddd;
    padding: 5px;

}

.left_content {   
    background-color: #ffffff;
    padding: 5px;
    margin-left: 220px;
    margin-bottom: 70px;
    height: 410px
}

.right_content {
    background-color: #dddddd;
    padding: 5px;
    margin: 0px 170px 0px 170px;
    
}

#top a:link, #top a:visited {
  color: white;
  font-weight : bold;
  text-decoration: none;
}

#top a:link:hover, #top a:visited:hover  {
  color: black;
  font-weight : bold;
  text-decoration : underline;
}


